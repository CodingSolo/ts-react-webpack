{
  "name": "ts-react-webpack",
  "version": "0.1.0",
  "description": "react application with typescript, webpack, babel",
  "main": "dist/bundle.js",
  "scripts": {
    "type-check": "./node_modules/.bin/tsc --noEmit && ./node_modules/.bin/eslint --ext ts,tsx ./src",
    "type-check-watch": "yarn type-check --watch",
    "start": "webpack serve --config webpack.dev.js --progress --color",
    "build": "webpack --config webpack.prod.js --progress --color --mode production",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch --env=jsdom",
    "cypress:open": "cypress open",
    "prepare": "husky install"
  },
  "jest": {
    "transform": {
      "**/__tests__/**/*.[jt]s?(x)": "babel-jest"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.[jt]s?(x)": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "keywords": [
    "typescript",
    "react",
    "webpack",
    "babel"
  ],
  "author": "CodingSolo",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.5.1",
    "cypress": "^9.3.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.28.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.2.2",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.5"
  },
  "resolutions": {
    "lodash": "^4.17.19",
    "http-proxy": "^1.18.1",
    "webpack-dev-server/*/node-forge": ">=1.0.0",
    "eslint/glob-parent": ">=5.1.2",
    "**/follow-redirects": ">=1.14.7",
    "**/path-parse": ">=1.0.7",
    "**/tmpl": ">=1.0.5",
    "**/ansi-regex": "^5.0.1",
    "serialize-javascript": "^3.1.0",
    "**/nth-check": ">=2.0.1",
    "css-loader/*/nanoid": ">=3.1.31"
  }
}
