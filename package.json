{
  "name": "ts-react-webpack",
  "version": "0.1.0",
  "description": "react application with typescript, webpack, babel",
  "main": "dist/bundle.js",
  "scripts": {
    "type-check": "./node_modules/.bin/tsc --noEmit && ./node_modules/.bin/eslint --ext ts,tsx ./src",
    "type-check-watch": "yarn type-check --watch",
    "start": "webpack serve --config webpack.dev.js --progress --color",
    "build": "webpack --config webpack.prod.js --progress --color --mode production",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch --env=jsdom",
    "cypress:open": "cypress open",
    "prepare": "husky install"
  },
  "jest": {
    "transform": {
      "**/__tests__/**/*.[jt]s?(x)": "babel-jest"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.[jt]s?(x)": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "keywords": [
    "typescript",
    "react",
    "webpack",
    "babel"
  ],
  "author": "CodingSolo",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-jest": "^27.0.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.0",
    "cypress": "^7.6.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "style-loader": "^3.0.0",
    "webpack": "^5.55.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.3.4"
  },
  "resolutions": {
    "lodash": "^4.17.19",
    "http-proxy": "^1.18.1",
    "node-forge": "^0.10.0",
    "serialize-javascript": "^3.1.0"
  }
}
