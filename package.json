{
  "name": "ts-react-webpack",
  "version": "0.1.0",
  "description": "react application with typescript, webpack, babel",
  "main": "dist/bundle.js",
  "scripts": {
    "type-check": "tsc --noEmit && eslint --ext ts,tsx ./src",
    "type-check-watch": "yarn type-check --watch",
    "start": "webpack-dev-server --config webpack.dev.js --progress --colors --open  --hot --mode development",
    "build": "webpack --config webpack.prod.js --progress --colors --mode production"
  },
  "keywords": [
      "typescript",
      "react",
      "webpack",
      "babel"
    ],
  "author": "CodingSolo",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.3.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.19.1",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@types/react": "^16.9.15",
    "@types/react-dom": "^16.9.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^3.7.3"
  }
}
